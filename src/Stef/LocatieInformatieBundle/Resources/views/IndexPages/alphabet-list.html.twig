<ul class="list-unstyled list-inline">
    <li><a href="{{ path(route, {'char' : 'apostrof'}) }}">{{ 'apostrof'|upper }}</a></li>
    {% for letter in 'a'..'z' %}
        <li><a href="{{ path(route, {'char' : letter}) }}" title="Locaties met letter {{ letter|upper }}">{{ letter|upper }}</a></li>
    {% endfor %}
</ul>